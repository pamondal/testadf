{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ppmadftest"
		},
		"salesourcedl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'salesourcedl'"
		},
		"intmSource_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'intmSource'"
		},
		"salesourcedl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ppmtestdl.dfs.core.windows.net"
		},
		"intmSource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ppmtestdl.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ppmadftest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"salesourcedl_accountKey": {
						"value": "[parameters('salesourcedl_accountKey')]"
					},
					"intmSource_accountKey": {
						"value": "[parameters('intmSource_accountKey')]"
					},
					"salesourcedl_properties_typeProperties_url": {
						"value": "[parameters('salesourcedl_properties_typeProperties_url')]"
					},
					"intmSource_properties_typeProperties_url": {
						"value": "[parameters('intmSource_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}